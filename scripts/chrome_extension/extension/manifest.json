
{
  "manifest_version": 3,
  "name": "Element Finder Extension",
  "version": "1.0",
  "permissions": [
    "tabs","activeTab", "scripting", "storage","webNavigation"
],
"host_permissions": [
  "<all_urls>"
],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "icons": {
      "16":  "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}